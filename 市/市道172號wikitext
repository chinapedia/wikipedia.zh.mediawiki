{{TWNint |location=岩前 |km=0.830 |road={{Jct|country=TWN|TRTN|96}} |notes=[[File:TW_THWtn96.svg|20px]]終點 }}
{{TWNint |location=坪頂 |km=7.601 |road={{Jct|country=TWN|CH|175}} |notes=公路終點 }}
{{Jctbtm |conv=no }}
