local z = {}

function z.pr( obj )
    if type( obj ) == 'table' then
        local rows = { '<table class="wikitable">' }
        for key, value in pairs( obj ) do
            table.insert( rows, '<tr><td>' .. key .. '</td><td>' .. z.pr( value ) .. '</td></tr>' )
        end
        table.insert( rows, '</table>' )
        return table.concat( rows )
    else
        return tostring( obj )
    end
end

return z