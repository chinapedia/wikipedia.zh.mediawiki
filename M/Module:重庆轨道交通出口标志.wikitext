local p = {}

function p.main(frame)
	
	local code = frame:getParent().args[1]
	local len = mw.ustring.len(code)
	local seq = ''
	
	for i = 1, len do
		local c = mw.ustring.sub(code, i, i)
		
		if c == 'Q' then
			seq = seq..'[[File:CRT_R_B_'..c..'.svg|x20px]]'
		else
			seq = seq..'[[File:CRT_R_B_'..c..'.svg|x16px]]'
		end
	end
	
	return seq
	
end

return p