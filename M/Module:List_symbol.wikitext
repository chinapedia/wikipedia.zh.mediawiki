local p = {}
function p.arrange(frame)
	local args = frame.args
	if args[1] == nil then
 		args = frame:getParent().args
	end

	local symbol = args.symbol or 'ï¼Ž'
	local ss = ''
	if args[1] ~= nil then
		ss = ss .. '<span style="white-space:nowrap">' .. args[1]
	end
	local i = 2
	while args[i] ~= nil do
		ss = ss .. ' ' .. symbol .. ' </span><span style="white-space:nowrap">' .. args[i]
		i = i + 1
	end
	ss = ss .. '</span>'
	ss = string.gsub(ss,'\n','')
	return ss
end

return p