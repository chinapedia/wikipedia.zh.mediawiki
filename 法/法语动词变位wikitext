| '''nous'''</span> || parl-<span style="color:green;">ons</span> <br>{{IPA|/paʁl}}<span style="color:green;">{{IPA|ɔ̃}}</span>{{IPA|/}} || parl-<span style="color:green;">âmes</span> <br>{{IPA|/paʁl}}<span style="color:green;">{{IPA|ɑm}}</span>{{IPA|/}} || parl-<span style="color:green;">ions</span> <br>{{IPA|/paʁl}}<span style="color:green;">{{IPA|jɔ̃}}</span>{{IPA|/}} || parl-<span style="color:green;">erons</span> <br>{{IPA|/paʁl}}<span style="color:green;">{{IPA|əʁɔ̃}}</span>{{IPA|/}} || parl-<span style="color:green;">ions</span> <br>{{IPA|/paʁl}}<span style="color:green;">{{IPA|jɔ̃}}</span>{{IPA|/}} || parl-<span style="color:green;">assions</span> <br>{{IPA|/paʁl}}<span style="color:green;">{{IPA|asjɔ̃}}</span>{{IPA|/}} || parl-<span style="color:green;">erions</span> <br>{{IPA|/paʁl}}<span style="color:green;">{{IPA|əʁjɔ̃}}</span>{{IPA|/}} || parl-<span style="color:green;">ons</span> <br>{{IPA|/paʁl}}<span style="color:green;">{{IPA|ɔ̃}}</span>{{IPA|/}}
