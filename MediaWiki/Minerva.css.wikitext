/* Mediawiki:Mobile.css */
/* Fix style of mw-highlight */
div.mw-highlight {
    background-color: #f8f8f8;
}
.mw-highlight pre {
    position: unset;
}
/* Fix style of dd scroll bar */
dl dd {
    overflow: unset;
}

/* Fix style of contributions list */
ul.mw-contributions-list .comment, ul.special .comment {
    word-break: break-word;
}