li#ca-0,
li#ca-1,
li#ca-2{
    display:block !important;
}

li#ca-varlang-0,
li#ca-varlang-1,
li#ca-varlang-2{
    display:inline !important;
}

li#ca-varlang-3{
    margin-left: 0em; !important;
}