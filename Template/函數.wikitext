<table width="250" class="floatright wikitable">{{
 #if: {{{name|}}}     | <tr><td colspan="2" style="background-color:#e0e0e0"><center>'''{{{name}}}'''</center></td></tr> }}{{
 #if: {{{image |}}}   | <tr><td colspan="2"><center>[[Image:{{{image}}}|220px]]</center>{{#if: {{{image-caption|}}} |<br/>{{{image-caption}}} }}</td></tr> }}{{
 #if: {{{heading1|}}} | <tr><td colspan="2" style="background-color:#e0e0e0"> ''性質'' </td></tr> }}{{
 #if: {{{parity |}}}  | <tr><td width="160">        [[奇函數與偶函數|奇偶性]] </td><td> {{{parity}}} </td></tr> }}{{
 #if: {{{domain|}}}   | <tr><td width="160">        [[定义域|定義域]] </td><td> {{{domain}}} </td></tr> }}{{
 #if: {{{codomain|}}} | <tr><td width="160">        [[到达域|到達域]] </td><td> {{{codomain}}} </td></tr> }}{{
 #if: {{{period|}}}   | <tr><td width="160">        [[周期函数|周期]] </td><td> {{{period}}} </td></tr> }}{{
 #if: {{{heading2|}}} | <tr><td colspan="2" style="background-color:#e0e0e0"> ''特定值'' </td></tr> }}{{
 #if: {{{zero|}}}     | <tr><td width="160">        [[截距|當x=0]]</td><td> {{{zero}}} </td></tr> }}{{
 #if: {{{plusinf|}}}  | <tr><td width="160">        當x=+∞ </td><td> {{{plusinf}}} </td></tr> }}{{
 #if: {{{minusinf|}}} | <tr><td width="160">        當x=-∞ </td><td> {{{minusinf}}} </td></tr> }}{{
 #if: {{{max|}}} | <tr><td width="160"> 最大值 </td><td> {{{max}}} </td></tr> }}{{
 #if: {{{min|}}} | <tr><td width="160"> 最小值 </td><td> {{{min}}} </td></tr> }}{{
 #if: {{{vr1|}}} | <tr><td width="160"> 當x={{{vr1}}} </td><td> {{{f1}}} </td></tr> }}{{
 #if: {{{vr2|}}} | <tr><td width="160"> 當x={{{vr2}}} </td><td> {{{f2}}} </td></tr> }}{{
 #if: {{{vr3|}}} | <tr><td width="160"> 當x={{{vr3}}} </td><td> {{{f3}}} </td></tr> }}{{
 #if: {{{vr4|}}} | <tr><td width="160"> 當x={{{vr4}}} </td><td> {{{f4}}} </td></tr> }}{{
 #if: {{{vr5|}}} | <tr><td width="160"> 當x={{{vr5}}} </td><td> {{{f5}}} </td></tr> }}{{
 #if: {{{heading3|}}} | <tr><td colspan="2" style="background-color:#e0e0e0"> ''其他性質'' </td></tr> }}{{
 #if: {{{asymptote|}}} | <tr><td width="160"> [[渐近线]] </td><td> {{{asymptote}}} </td></tr> }}{{
 #if: {{{root|}}} | <tr><td width="160">      [[根 (数学)|根]] </td><td> {{{root}}} </td></tr> }}{{
 #if: {{{critical|}}} | <tr><td width="160">  [[临界点 (数学)|臨界點]]</td><td> {{{critical}}} </td></tr> }}{{
 #if: {{{inflection|}}} | <tr><td width="160">[[拐点|拐點]]</td><td> {{{inflection}}} </td></tr> }}{{
 #if: {{{fixed|}}} | <tr><td width="160">     [[不动点|不動點]]</td><td> {{{fixed}}} </td></tr> }}{{
 #if: {{{notes|}}} | <tr><td colspan="2"><small>{{{notes}}}</small></td></tr> }}</table><noinclude>{{doc}}</noinclude>