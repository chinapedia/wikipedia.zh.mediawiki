<center>
{{#tag:timeline|
DateFormat=dd/mm/yyyy
Define $today = {{#time:d/m/Y}}
ImageSize= width:800 height:auto barincrement:30
Period   = from:01/01/1969 till:$today
TimeAxis = orientation:horizontal
PlotArea = right:20 left:20 bottom:30 top:10

Colors =
     id:bg         value:gray(0.95)
     id:current    value:rgb(0.7,0.9,0.7)
     id:previous   value:rgb(1,0.8,0.7)
     id:line       value:black
     id:lightline  value:rgb(0.8,0.8,0.8)
     id:header     value:rgb(0.6,0.6,0.9)
     id:lighttext  value:rgb(0.5,0.5,0.5)
     id:retired    value:orange
     id:active     value:green

BackgroundColors = canvas:bg
ScaleMajor = gridcolor:line unit:year increment:5 start:01/01/1970
ScaleMinor = gridcolor:lightline unit:year increment:1 start:01/01/1970

BarData = 
  Bar:Title
  Barset:LM1
  Barset:LM2
  Barset:LM2E
  Barset:LM3
  Barset:LM4
  Barset:LM5
  Barset:LM6
  Barset:LM7
  Barset:LM8
  Barset:LM11

PlotData=
  width:18 textcolor:black align:center fontsize:m shift:(0,-5)

  bar:Title color:header from:16/11/1969 till:$today text:"長征系列運載火箭"

  barset:LM1
    color:retired  from:16/11/1969 till:03/03/1971 text:"[[長征一號運載火箭|長征一號]]"
    barset:break
    color:retired  from:01/06/1995 till:03/01/2002 text:"[[長征一號丁運載火箭|長征一號丁]]"
  barset:LM2
    color:retired  from:05/11/1974 till:26/01/1978 text:"[[長征二號運載火箭|長征二號]]"
    barset:break
    color:active   from:09/09/1982 till:$today     text:"[[长征二号丙运载火箭|长征二号丙]]"
    color:active   from:09/08/1992 till:$today     text:"[[长征二号丁运载火箭|长征二号丁]]"
  barset:LM2E
    color:retired  from:16/07/1990 till:28/12/1995 text:"[[长征二号E运载火箭|长征二号E]]"
    barset:break
    color:active   from:19/11/1999 till:$today     text:"[[长征二号F运载火箭|长征二号F]]"
  barset:LM3
    color:retired  from:29/01/1984 till:25/06/2000 text:"[[長征三號運載火箭|長征三號]]"
    color:active   from:08/02/1994 till:$today     text:"[[长征三号甲运载火箭|长征三号甲]]"
    color:active   from:14/02/1996 till:$today     text:"[[长征三号乙运载火箭|长征三号乙]]"
    color:active   from:25/04/2008 till:$today     text:"[[长征三号丙运载火箭|长征三号丙]]"
  barset:LM4
    color:retired  from:06/09/1988 till:03/09/1990 text:"[[长征四号甲运载火箭|长征四号甲]]"
    barset:break
    color:active   from:10/05/1999 till:$today     text:"[[长征四号乙运载火箭|长征四号乙]]"
    color:active   from:26/04/2006 till:$today     text:"[[长征四号丙运载火箭|长征四号丙]]"
  Barset:LM5
    color:active   from:03/11/2016 till:$today     text:"[[长征五号系列火箭|长征五号]]"
    color:active   from:05/05/2020 till:$today     text:"[[长征五号B运载火箭|长征五号B]]" align:right
  Barset:LM6
    color:active   from:19/09/2015 till:$today     text:"[[长征六号运载火箭|长征六号]]"
    color:active   from:29/03/2022 till:$today     text:"[[长征六号甲运载火箭|长征六号甲]]"
  Barset:LM7
    color:active   from:25/06/2016 till:$today     text:"[[长征七号运载火箭|长征七号]]"
    color:active   from:16/03/2020 till:$today     text:"[[长征七号改运载火箭|长征七号改]]" align:right
  Barset:LM8
    color:active   from:22/12/2020 till:$today     text:"[[长征八号运载火箭|长征八号]]" align:right
  Barset:LM11
    color:active   from:25/09/2015 till:$today     text:"[[长征十一号运载火箭|长征十一号]]"
}}
