{{Infobox
| title = {{If empty|{{{name|{{{tournament_name|}}}}}}|{{PAGENAMEBASE}}}}

| headerstyle = background:lavender
| subheaderstyle = background:lavender
| subheader = {{#if:{{{year|}}}|{{Endwithyear|{{{year}}}}} }}

| image = {{#invoke:InfoboxImage|InfoboxImage|image={{{image|}}}|size={{{image_size|}}}|sizedefault=frameless|alt={{{alt|}}}}}
| caption = {{{caption|}}}

| header1 = <includeonly>赛事资料</includeonly>

| labelstyle = white-space:nowrap;

| label2 = 赛事
| data2 = {{{sport|}}}

| label3 = 地点
| data3 = {{{location|}}}

| label4 = 日期
| data4  = {{#if:{{{date|}}}|{{{date}}}|{{#if:{{{start_date|}}} | {{{start_date}}}－{{{end_date|}}} }}}}

| label5 = Month played
| data5 = {{{month_played|{{{month played|}}}}}}

| label6 = 开始
| data6 = {{{establishment|}}}

| label7 = 结束
| data7 = {{{defunct|}}} 

| label8 = 最终年份
| data8 = {{{final_year|{{{final year|}}} }}} 

| label9 = 比赛数
| data9 = {{{number|}}}

| label10  = 主办方
| data10 = {{{administrator|{{{administrators|}}}}}}

| label11 = 形式
| data11 = {{{format|}}}

| label12 = 比赛形式
| data12  = {{{tournament_format|{{{tournament format|}}}}}}

| label13 = 主持人
| data13  = {{{host|}}}

| label14 = 场馆
| data14  = {{{venues|}}}

| label15 = {{#if:{{{participants|}}}|参与者|队伍}}
| data15  = {{#if:{{{participants|}}}|{{{participants}}}|{{{teams|}}}}}

| label16 = 资金
| data16  = {{{purse|}}}

| label17 = 广播
| dat17   = {{{broadcast|}}}

| label18 = 网站
| data18  = {{{website|}}}

| header19 = {{#if:{{{champion|}}}{{{champions|}}}{{{runner-up1|}}}{{{runners-up1|}}}{{{runner-up2|}}}{{{runners-up2|}}}|最终排名}}

| label20  = 冠军
| data20   = {{#if:{{{champion|}}}|{{{champion}}}|{{{champions|}}}}}

| label21  = 亚军
| data21   = {{#if:{{{runner-up1|}}}|{{{runner-up1}}}|{{{runners-up1|}}}}}

| label22  = 季军
| data22   = {{#if:{{{runner-up2|}}}|{{{runner-up2}}}|{{{runners-up2|}}}}}

| header23 = {{#if:{{{matches|}}}{{{goals|}}}{{{attendance|}}}{{{top_scorer|}}}{{{best_player|}}}{{{stat1|}}}{{{stat2|}}}|比赛统计}}

| label24  = 比赛总场次
| data24   = {{{matches|}}}

| label25  = Goals&nbsp;scored
| data25   = {{{goals|}}} {{#if:{{both|{{{matches|}}}|{{{goals|}}}}}|{{#iferror:{{#expr:{{{goals|}}}/{{{matches|}}}}}||{{small|({{#expr:{{{goals|}}}/{{{matches|}}} round 2}} per match)}}}}}}

| label26  = Points&nbsp;scored
| data26   = {{{points|}}} {{#if:{{both|{{{matches|}}}|{{{points|}}}}}|{{#iferror:{{#expr:{{{points|}}}/{{{matches|}}}}}||{{small|({{#expr:{{{points|}}}/{{{matches|}}} round 2}} per match)}}}}}}

| label27  = Tries&nbsp;scored
| data27   = {{{tries|}}} {{#if:{{both|{{{matches|}}}|{{{tries|}}}}}|{{#iferror:{{#expr:{{{tries|}}}/{{{matches|}}}}}||{{small|({{#expr:{{{tries|}}}/{{{matches|}}} round 2}} per match)}}}}}}

| label28  = 出席人数
| data28   = {{#if:{{{attendance|}}}|{{formatnum: {{{attendance|0}}}}}{{#ifeq:{{{noaverage|}}}|1||{{#if:{{{matches|}}}|&nbsp;<small>({{formatnum: {{#expr: {{{attendance|0}}} / {{{matches|1}}} round 0}}}} per match)</small>}}}}}}

| label29 = {{#if:{{{mvp_label|}}}|{{{mvp_label}}}|[[最有价值球员|MVP]]}}
| data29  = {{{mvp|}}}

| label30 = 最高得分者
| data30  = {{{top_scorer|}}}

| label31 = Top&nbsp;try&nbsp;scorer(s)
| data31  = {{{top_try_scorer|}}}

| label32 = {{{stat1_label}}}
| data32  = {{#if:{{{stat1_label}}}| {{{stat1|}}} }}

| label33 = {{{stat2_label}}}
| data33  = {{#if:{{{stat2_label}}}| {{{stat2|}}} }}

| header35 = {{#if: {{{current_champion|{{{Current Champion|}}}}}}|当前冠军 }}

| data36 = {{{current_champion|{{{Current Champion|}}}}}}

| header37 = {{#if:{{{current|}}}|最近比赛}}
| data38 = {{{current|}}}

| header40 = {{#if: {{{final_champion|{{{Final Champion|}}}}}}| 最终冠军 }}

| data41 = {{{final_champion|{{{Final Champion|}}}}}}

| belowstyle = border-top:1px #aaa solid
| below  = {{#if:{{{previous|}}}{{{next|}}}| {{succession links| left = {{{previous|}}}| right = {{{next|}}} }} }}

}}<noinclude>
{{documentation}}
</noinclude>