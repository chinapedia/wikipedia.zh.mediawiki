|groupstyle = text-align: center;

|group1 = <del>[[呼倫貝爾盟 (1948年－1949年)|呼伦贝尔盟]]</del>
|list1 = 1948年成立；1949年4月11日撤销，辖域并入新设立的[[呼伦贝尔纳文慕仁盟]]。

|group2 = <del>[[纳文慕仁盟]]</del>
|list2 = 1949年4月11日撤销，辖域并入新设立的[[呼伦贝尔纳文慕仁盟]]。

|group3 = <del>[[呼伦贝尔纳文慕仁盟]]</del>
|list3 = 1949年4月11日成立；1953年4月1日撤销，辖域并入新设立的[[内蒙古自治区东部区行政公署]]。

|group4 = <del>[[呼伦贝尔盟]]</del>
|list4 = 1954年5月21日撤销[[内蒙古自治区东部区行政公署]]，按[[兴安盟]]与[[呼伦贝尔纳文慕仁盟]]原辖域合并成立呼伦贝尔盟；1980年7月26日恢复析置[[兴安盟]]；2001年10月10日撤销呼伦贝尔盟设立地级[[呼伦贝尔市]]，含代管市辖域未变。

|group5 = <del>[[察哈尔盟]]</del>
|list5 = 1958年9月26日撤销，辖域并入[[锡林郭勒盟]]。

|group6 = <del>[[昭乌达盟]]</del>
|list6 = 1983年10月10日撤销，设立地级[[赤峰市]]。

|group7 = <del>[[哲里木盟]]</del>
|list7 = 1953年4月1日同时撤销，辖域入新设立的[[内蒙古自治区东部区行政公署]]；1954年5月21日撤销[[内蒙古自治区东部区行政公署]]后按原辖域恢复成立；1999年1月13日撤销，成立地级[[通辽市]]，辖域有调整。

|group8 = <del>[[伊克昭盟]]</del> 
|list8 = 1954年6月19日撤销伊克昭蒙古族自治区，设立伊克昭盟；2001年2月26日撤销，设立地级[[鄂尔多斯市]]。

|group9 = <del>[[巴彦淖尔盟]]</del> 
|list9 = 1956年4月3日撤销巴彦浩特蒙古族自治州，设立巴彦淖尔盟；2003年12月1日撤销，设[[巴彦淖尔市]]，辖域为撤销前的“巴彦淖尔盟”辖域。

|group10 = <del>[[乌兰察布盟]]</del>
|list10 = 1954年6月19日撤销乌兰察布蒙古族自治区，设立乌兰察布盟；2003年12月1日撤销，设[[乌兰察布市]]，含代管县级市（丰镇市）辖域实际未变。

|group11 = [[锡林郭勒盟]]
|list11 = 1958年9月26日撤销[[察哈尔盟]]，原察哈尔盟辖域并入锡林郭勒盟。

|group12 = [[阿拉善盟]]
|list12 = 1979年12月12日设置。

|group13 = [[兴安盟]] 
|list13 = 1953年4月1日撤销，辖域并入新设立的[[内蒙古自治区东部区行政公署]]；1954年5月21日撤销内蒙古自治区东部区行政公署，按兴安盟与[[呼伦贝尔纳文慕仁盟|呼纳盟]]原辖域合并成立[[呼伦贝尔盟]]；1980年7月26日按原辖域从呼伦贝尔盟分出恢复设置。
