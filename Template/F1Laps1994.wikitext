<timeline>
Preset     = TimeVertical_OneBar_UnitYear
ImageSize  = width:250 height:300
PlotArea   = bottom:20 left:40
Period     = from:0 till:58
ScaleMajor = start:0 increment:5
ScaleMinor = start:1 increment:1

Colors =
  id:canvas      value:rgb(0.937,0.937,0.937)
  id:linemark    value:gray(0.8)
  id:linemark2   value:gray(0.9)

BackgroundColors = canvas:canvas

PlotData =
  mark:(line,linemark)
  from:0 till:5    color:darkblue  shift:(20,-5)  text:埃尔顿·塞纳（Laps 1–5）
  from:5 till:12   color:green     shift:(20,-5)  text:迈克尔·舒马赫（Laps 6–12）
  from:12 till:15  color:red       shift:(20,-5)  text:加赫特·贝加（Laps 13–15）
  from:15 till:18  color:yellow    shift:(20,-5)  text:米卡·哈基宁（Laps 16–18）
  from:18 till:58  color:green     shift:(20,-5)  text:迈克尔·舒马赫（Laps 19–58）
</timeline>
