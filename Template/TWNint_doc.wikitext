| cspan = 4
| tcd = 永康區
| tspan = 2
| location = 六甲頂
| km = 330.700
| road = {{Jct|country=TWN|PH|19}}
| notes = This is an example.
}}
{{TWNint
| location = 大橋
| km = 332.450
| road = {{Jct|country=TWN|PH|20}}
| notes = This is an example.
}}
{{TWNjctco|tcd=南區 (臺南市){{!}}南區}}
{{TWNint
| tcd = 仁德區
| location = [[台南交流道 (台86線)|台南交流道]]
| km = 343.000
| road = {{Jct|country=TWN|PH|86}}
| notes = This is an example.
}}
{{TWNint
| county = 高雄市
| cspan = 1
| tcd = 湖內區
| location = 大湖
| km = 349.400
| road = {{Jct|country=TWN|PH|28}}
| notes = This is an example.
}}
{{Jctbtm|conv=no}}
</nowiki>}}

