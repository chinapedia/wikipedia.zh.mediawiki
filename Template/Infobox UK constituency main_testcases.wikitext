{{Infobox
|name         = <!-- Infobox UK constituency main -->
|decat = yes
|bodyclass    = vcard <!-- for Wikipedia:WikiProject Microformats -->

|bodystyle    = width: 28em; <!-- added by BrownhairedGirl 6 July 2010. Was unspecified, and default infobox with is currently 22em  -->
|captionstyle =<!-- font-size: 80%; -->

|headerstyle    = background: #ccf;
|subheaderstyle = font-weight: bold;
|labelstyle     = background: #ddf; width: 50%
|datastyle      =
|belowstyle     = background: #ddf;

<!-- headers -->
|aboveclass   = fn org
|above        = {{{name}}}
|subheader    = {{#if:{{{abolished|}}}|前|}} {{#switch:{{{parliament|uk}}}
   |uk=[[{{{type}}}選區]]
   |#default=constituency}}
|subheaderclass = category
|subheader2   = {{#switch:{{{parliament|uk}}}
   |uk=[[英國下議院|下議院]]
   |#default=未知{{{parliament}}}}}

<!-- images -->
|image        = {{#if:{{{map1|}}}|[[File:{{{map1}}}Constituency.svg|{{#if:{{{map_size|}}}|{{{map_size}}}|120px}}|alt=Outline map]]|{{{image|}}}}}
