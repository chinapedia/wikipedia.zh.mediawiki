|image3       = {{#if:{{{map3|}}}|[[File:{{{map3}}}.svg|{{#if:{{{map_size|}}}|{{{map_size}}}|120px}}|alt=Outline map]]|{{{image3|}}}}}
|caption3     = {{#if:{{{map3|}}}|{{{map_entity}}}在{{{region}}}的位置|{{{caption3|}}}}}

<!-- main content -->
