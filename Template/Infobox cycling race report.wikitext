<includeonly>{{Infobox
| bodyclass = vevent
| bodystyle = min-width: 22em
| title = {{{name|}}}
| abovestyle = font-size:90%;font-weight:normal;
| above = {{{series|}}}{{#if:{{{race_no|}}}|{{#if:{{{season_no|}}}|第{{{season_no}}}個賽季的}}第{{{race_no}}}站}}
| headerstyle = background:#CCCCFF;
| image = {{#invoke:InfoboxImage|InfoboxImage|image= {{{image|}}}|size= {{{image_size|}}}|alt= {{{image_alt|}}}|title= {{{image_caption|}}}}}
| caption = {{{image_caption|}}}

| header1 = 賽事細節
| label2 = 日期
| data2 = {{{date|}}}
| label3 = 賽段
| data3 = {{{stages|}}}
| label4 = 距離
| data4 = {{#if:{{{distance|}}}|{{convert|{{{distance|}}}|{{{unit|}}}|abbr=on|sigfig=4}}}}
| label5 = 冠軍用時
| data5 = {{{time|}}}

| header6 = {{#if:{{{first|}}}{{{second|}}}{{{third|}}}{{{points|}}}{{{mountains|}}}{{{youth|}}}{{{combination|}}}{{{sprints|}}}{{{combativity|}}}{{{intergiro|}}}{{{team|}}}{{{teampoints|}}}|{{#switch: {{{type}}}| medals = 獎牌榜 | 賽果}}}}

| data7 = {{#if:{{{first|}}}{{{second|}}}{{{third|}}}{{{points|}}}{{{mountains|}}}{{{youth|}}}{{{combination|}}}{{{sprints|}}}{{{combativity|}}}{{{intergiro|}}}{{{team|}}}{{{teampoints|}}}|{{infobox3cols
  | labelstyle = white-space:nowrap; width:20px; text-align:left;
  | datastylea =  white-space:nowrap; font-weight: bold; text-align:left;
  | datastyleb =  white-space:nowrap; text-align:left;
  | datastylec =  white-space:nowrap; text-align:left;
  | child = yes
  | label1 = {{#if:{{{first|}}}|{{#if:{{{first_color|}}}|[[File:Jersey {{{first_color}}}.svg|20px|Jersey awarded to the overall winner|alt=|link=]]|&nbsp;}}}}
  | data1a = {{#switch: {{{type}}}| medals = {{gold medal}} | 冠軍}}
  | data1b = {{#if:{{{first_nat|}}}|{{flagathlete|{{{first}}}|{{{first_nat}}}|{{{first_natvar}}}}}|{{{first}}}}}
  | data1c = {{#if:{{{first_team|}}}|({{{first_team|}}})}}
  | label2 = {{#if:{{{second|}}}|&nbsp;}}
  | data2a = {{#switch: {{{type}}}| medals = {{silver medal}} | 亞軍}}
  | data2b = {{#if:{{{second_nat|}}}|{{flagathlete|{{{second}}}|{{{second_nat}}}|{{{second_natvar}}}}}|{{{second}}}}}
  | data2c = {{#if:{{{second_team|}}}|({{{second_team|}}})}}
  | label3 = {{#if:{{{third|}}}|&nbsp;}}
  | data3a = {{#switch: {{{type}}}| medals = {{bronze medal}} | 季軍}}
  | data3b = {{#if:{{{third_nat|}}}|{{flagathlete|{{{third}}}|{{{third_nat}}}|{{{third_natvar}}}}}|{{{third}}}}}
  | data3c = {{#if:{{{third_team|}}}|({{{third_team|}}})}}
  | header4 = {{#if:{{{first|}}}{{{second|}}}{{{third|}}}|{{#if:{{{points|}}}{{{mountains|}}}{{{youth|}}}{{{combination|}}}{{{sprints|}}}{{{combativity|}}}{{{intergiro|}}}{{{team|}}}{{{teampoints|}}}|{{hr1}}}}}}
  | label5 = {{#if:{{{points|}}}|{{#if:{{{points_color|}}}|[[File:Jersey {{{points_color}}}.svg|20px|Jersey awarded to the points classification winner|alt=|link=]]|&nbsp;}}}}
  | data5a = 積分王
  | data5b = {{#if:{{{points_nat|}}}|{{flagathlete|{{{points}}}|{{{points_nat}}}|{{{points_natvar}}}}}|{{{points}}}}}
  | data5c = {{#if:{{{points_team|}}}|({{{points_team}}})}}
  | label6 = {{#if:{{{mountains|}}}|{{#if:{{{mountains_color|}}}|[[File:Jersey {{{mountains_color}}}.svg|20px|Jersey awarded to the mountains classification winner|alt=|link=]]|&nbsp;}}}}
  | data6a = 登山王
  | data6b = {{#if:{{{mountains_nat|}}}|{{flagathlete|{{{mountains}}}|{{{mountains_nat}}}|{{{mountains_natvar}}}}}|{{{mountains}}}}}
  | data6c = {{#if:{{{mountains_team|}}}|({{{mountains_team}}})}}
  | label7 = {{#if:{{{youth|}}}|{{#if:{{{youth_color|}}}|[[File:Jersey {{{youth_color}}}.svg|20px|Jersey awarded to the youth classification winner|alt=|link=]]|&nbsp;}}}}
  | data7a = 青年冠軍
  | data7b = {{#if:{{{youth_nat|}}}|{{flagathlete|{{{youth}}}|{{{youth_nat}}}|{{{youth_natvar}}}}}|{{{youth}}}}}
  | data7c = {{#if:{{{youth_team|}}}|({{{youth_team}}})}}
  | label8 = {{#if:{{{combination|}}}|{{#if:{{{combination_color|}}}|[[File:Jersey {{{combination_color}}}.svg|20px|Jersey awarded to the combination classification winner|alt=|link=]]|&nbsp;}}}}
  | data8a = 混合冠軍
  | data8b = {{#if:{{{combination_nat|}}}|{{flagathlete|{{{combination}}}|{{{combination_nat}}}|{{{combination_natvar}}}}}|{{{combination}}}|&nbsp;}}
  | data8c = {{#if:{{{combination_team|}}}|({{{combination_team}}})}}
  | label9 = {{#if:{{{sprints|}}}|{{#if:{{{sprints_color|}}}|[[File:Jersey {{{sprints_color}}}.svg|20px|Jersey awarded to the sprints classification winner|alt=|link=]]|&nbsp;}}}}
  | data9a = 衝刺王
  | data9b = {{#if:{{{sprints_nat|}}}|{{flagathlete|{{{sprints}}}|{{{sprints_nat}}}|{{{sprints_natvar}}}}}|{{{sprints}}}}}
  | data9c = {{#if:{{{sprints_team|}}}|({{{sprints_team}}})}}
  | label10 = {{#if:{{{combativity|}}}|{{#if:{{{combativity_color|}}}|[[File:Jersey {{{combativity_color}}}.svg|20px|Jersey awarded to the combativity classification winner|alt=|link=]]|&nbsp;}}}}
  | data10a = 奮戰獎
  | data10b = {{#if:{{{combativity_nat|}}}|{{flagathlete|{{{combativity}}}|{{{combativity_nat}}}|{{{combativity_natvar}}}}}|{{{combativity}}}}}
  | data10c = {{#if:{{{combativity_team|}}}|({{{combativity_team}}})}}
  | label11 = {{#if:{{{intergiro|}}}|{{#if:{{{intergiro_color|}}}|[[File:Jersey {{{intergiro_color}}}.svg|20px|Jersey awarded to the intergiro classification winner|alt=|link=]]|&nbsp;}}}}
  | data11a = Intergiro獎
  | data11b = {{#if:{{{intergiro_nat|}}}|{{flagathlete|{{{intergiro}}}|{{{intergiro_nat}}}|{{{intergiro_natvar}}}}}|{{{intergiro}}}}}
  | data11c = {{#if:{{{intergiro_team|}}}|({{{intergiro_team}}})}}
  | label12 = {{#if:{{{team|}}}|{{#if:{{{team_color|}}}|[[File:Jersey {{{team_color}}}.svg|20px|Jersey awarded to the team classification winner|alt=|link=]]|&nbsp;}}}}
  | data12a = 團隊總冠軍
  | data12b = {{{team|}}}
  | label13 = {{#if:{{{teampoints|}}}|&nbsp;}}
  | data13a = 團隊積分
  | data13b = {{{teampoints|}}}
}}}}
| below = {{#if: {{{previous|}}}|{{align|left|&larr;&nbsp;{{auto link|{{{previous|}}} }} }}}}{{#if: {{{next|}}}|{{align|right|{{auto link|{{{next|}}}}}&nbsp;&rarr; }}}}
| belowclass = hlist noprint nowrap
| belowstyle = border-top: 1px solid #aaa; padding-top: 3px;
