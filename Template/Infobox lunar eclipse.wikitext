<includeonly>{{Infobox
| bodyclass      = vcard
| title          = {{PAGENAME}}
| subheaderstyle = font-size: 100%; {{#switch:{{lc:{{{type|}}}}}
  | total        = background-color: #690005
  | partial      = background-color: #c56b55
  | penumbral    = background-color: #727272
  }}
| subheader1     = {{#switch:{{lc:{{{type|}}}}}
  | total        = {{Color|white|月全食}}
  | partial      = {{Color|white|月偏食}}
  | penumbral    = {{Color|white|半影月食}}
  }}

| image      = {{#invoke:InfoboxImage|InfoboxImage|image={{{image|}}}|alt={{{alt|}}}}}
| caption    = {{{caption|}}}

| label1     = 日期
| data1      = {{{date|}}}
| label2     = [[伽瑪 (食)|伽馬]]
| data2      = {{{gamma|}}}
| label3     = [[食分]]
| data3      = {{{magnitude|}}}
| label4     = [[沙羅周期]]
| data4      = {{#if:{{{saros_ser|}}}|{{#ifexist:月食的第{{{saros ser|}}}沙羅週期|[[月食的第{{{saros ser|}}}沙羅週期|{{{saros_ser|}}}]]（{{{saros_no|}}}）|{{{saros_ser|}}}（{{{saros_no|}}}）}}|}}
| label6     = 月全食
| data6      = {{{totality|}}}
| label7     = 月偏食
| data7      = {{{partiality|}}}
| label8     = 半影月食
| data8      = {{{penumbral|}}}

| data10     = {{#if:{{{p1|}}}|{{Infobox
 | bodyclass  = collapsible collapsed
 | subbox     = yes
 | abovestyle = font-size:95%
 | above      = {{resize|105%|接觸（[[Coordinated Universal Time|UTC]]）}}
 | label1     = 半影食始（P1） 
 | data1      = {{{p1|}}}
 | label2     = 初虧（U1）
 | data2      = {{{u1|}}}
 | label3     = 食既（U2）
 | data3      = {{{u2|}}}
 | label4     = 食甚
 | data4      = {{{greatest|}}}
 | label5     = 生光（U3）
 | data5      = {{{u3|}}}
 | label6     = 復圓（U4）
 | data6      = {{{u4|}}}
 | label7     = 半影食終（P4）
 | data7      = {{{p4|}}}
 }}
}}

| below      = {{succession links
| left       = {{#if:{{{previous|}}}|{{#ifexist:{{{previous|}}}月食|[[{{{previous|}}}月食|{{{previous|}}}]]|{{{previous|}}}}}}}
| right      = {{#if:{{{previous|}}}|{{#ifexist:{{{next|}}}月食|[[{{{next|}}}月食|{{{next|}}}]]|{{{next|}}}}}}}
}}
}}</includeonly><noinclude>
