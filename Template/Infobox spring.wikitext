{{infobox
| bodyclass      = vcard
| bodystyle      = width: 298px;

| labelstyle     =
| datastyle      =

| child          = {{{child|{{{产物|{{{產物|}}}}}}}}}

| abovestyle     = background-color:#A8BDEC;
| aboveclass     = fn org
| above          = <includeonly>{{{name{{{名称|{{{名稱|}}}}}}}}}</includeonly>

| subheaderstyle = font-size:100%;
| subheader      = {{{other_name|{{{其他名称|{{{其他名稱|}}}}}}}}}

| image    = {{#invoke:InfoboxImage|InfoboxImage|image={{{photo|}}}|size={{{photo_width|}}}|maxsize=284px|alt={{{photo_alt|}}}}}
| caption  = {{{photo_caption|}}}

| data1    = {{#if:{{{photo|}}} || {{#if:{{{map|}}}
             | {{Infobox map
               | map = {{{map|}}}
               | map_alt = {{{map_alt|}}}
               | map_width = {{least|{{{map_width|}}}|220}}
               | map_caption = {{{map_caption|}}}
               | relief = {{{relief|}}}
               | mark = {{{mark|}}}
               | mark_width = {{{mark_width|}}}
               | label = {{{label|}}}
               | label_position = {{{label_position|}}}
               | lat = {{#Invoke:Coordinates|dms2dec<!--
                                -->|{{{lat_NS|N}}}|{{{lat_d|}}}|{{{lat_m|}}}|{{{lat_s|}}}}}
               | long = {{#Invoke:Coordinates|dms2dec<!--
                                -->|{{{long_EW|E}}}|{{{long_d|}}}|{{{long_m|}}}|{{{long_s|}}}}}
               | x% = {{{x%|}}} | y% = {{{y%|}}}
               | x = {{{x|}}} | y = {{{y|}}} }} }} }}

| class2   = label
| label2   = 名稱來源
| data2    = {{{name_origin|{{{名称来源|{{{名稱來源|}}}}}}}}}

| class3   = label
| label3   = 位置
| data3    = {{{location|{{{位置|}}}}}}

| label4  = [[地理座標系|座標]]
| data4   = {{#if:{{{lat_d|}}}{{{long_d|}}}
            | {{Infobox coord
              | lat_d   = {{{lat_d|}}}
              | lat_m   = {{{lat_m|}}}
              | lat_s   = {{{lat_s|}}}
              | lat_NS  = {{{lat_NS|}}}
              | long_d  = {{{long_d|}}}
              | long_m  = {{{long_m|}}}
              | long_s  = {{{long_s|}}}
              | long_EW = {{{long_EW|}}}
              | type    = landmark
              | region  = {{{region|}}}
              | dim     = {{{dim|}}}
              | scale   = {{{scale|}}}
              | source  = {{{source|}}}
              | format  = {{{format|}}}
              | display = {{{display|}}}
              | name    = <!-- leave undefined --> }}
            | {{{coords|}}}
            }}{{{coords_ref|}}}

| class5   = note
| label5   = 海拔
| data5    = {{{elevation|{{{海拔|}}}}}}

| class6   = note
| label6   = [[溫泉|類型]]
| data6    = {{{type|{{{類型|{{{類型|}}}}}}}}}

| class7   = note
| label7   = 噴發高度
| data7    = {{{height|{{{高度|}}}}}}

| class8   = note
| label8   = 頻率
| data8    = {{{frequency|{{{频率|{{{頻率|}}}}}}}}}

| class9   = note
| label9   = 持續時間
| data9    = {{{duration|{{{持续时间|{{{持續時間|}}}}}}}}}

| class10  = note
| label10  = 流量
| data10   = {{{discharge|{{{流量|{{{流量|}}}}}}}}}

| class11  = note
| label11  = 溫度
| data11   = {{{temperature|{{{溫度|}}}}}}

| class12  = note
| label12  = 深度
| data12   = {{{depth|{{{深度|}}}}}}

| class13  = note
| label13  = Commons
| data13   = {{{commons|}}}

| label14  = [[地形圖]]
| data14   = {{{topo|{{{地形图|{{{地形圖|}}}}}}}}}

| data15   = {{#if:{{{photo|}}} | {{#if:{{{map|}}}
             | {{Infobox map
               | map = {{{map|}}}
               | map_alt = {{{map_alt|}}}
               | map_width = {{least|{{{map_width|}}}|220}}
               | map_caption = {{{map_caption|}}}
               | relief = {{{relief|}}}
               | mark = {{{mark|}}}
               | mark_width = {{{mark_width|}}}
               | label = {{{label|}}}
               | label_position = {{{label_position|}}}
               | lat = {{decdeg|{{{lat_d|}}}|{{{lat_m|}}}|{{{lat_s|}}}|{{{lat_NS|}}} }}
               | long = {{decdeg|{{{long_d|}}}|{{{long_m|}}}|{{{long_s|}}}|{{{long_EW|}}} }}
               | x% = {{{x%|}}} | y% = {{{y%|}}}
               | x = {{{x|}}} | y = {{{y|}}} }} }} }}
| data16   = {{{embedded|}}}
}}<noinclude>

{{#ifeq:{{PAGENAME}}|{{BASEPAGENAME}}/sandbox
| {{template sandbox notice}}
| {{documentation}}
}}
<!--
-->
</noinclude>