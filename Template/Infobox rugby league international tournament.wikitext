{{ infobox
| bodyclass  = vevent
| bodystyle  = width:25em
| aboveclass = summary
| above      = {{#if:{{{title|}}}|{{Start date|{{{year|}}}}} {{{title|}}} {{Start date|{{{yearr|}}}}} <includeonly>|{{PAGENAME}}</includeonly>}}
| abovestyle = font-size: large
| image      = {{#if:{{{image|}}}|[[File:{{{image}}} {{#if:{{{imagesize|150px}}}|{{!}}{{{imagesize}}}}}{{#if:{{{alt|}}}|{{!}}alt={{{alt}}}}}]]}}

| caption    = {{{caption|}}}
| label1     = 參加隊數
| data1      = {{{finalists|}}} 
| label2     = 主辦國家{{#if:{{{country2|}}}}}
| data2      = {{#if:{{{country|}}}|{{flag|{{{country|}}}|{{{country-flagvar|}}} }} }}<!--
  -->{{#if:{{{country2|}}}|<br />{{flag|{{{country2|}}}|{{{country2-flagvar|}}} }} }}<!--
  -->{{#if:{{{country3|}}}|<br />{{flag|{{{country3|}}}|{{{country3-flagvar|}}} }} }}<!--
  -->{{#if:{{{country4|}}}|<br />{{flag|{{{country4|}}}|{{{country4-flagvar|}}} }} }}<!--
  -->{{#if:{{{country5|}}}|<br />{{flag|{{{country5|}}}|{{{country5-flagvar|}}} }} }}
| label3     = 冠軍
| data3      = {{#if:{{{winners|}}}|{{rl|{{{winners|}}}|{{{winners-flagvar|}}}}}{{#if: {{{count|}}}|{{nowrap|<small> ({{ordinal|{{{count}}}}} title)</small>}} }} }}

| label4     = 冠軍<!-- Non Template:rl input -->
| data4      = {{#if:{{{winners_other|}}}|{{{winners_other}}}{{#if: {{{count|}}}|{{nowrap|<small> ({{ordinal|{{{count}}}}} title)</small>}} }} }}

| label5     = 亞軍
| data5      = {{#if:{{{runnerup|}}}|{{rl|{{{runnerup|}}}|{{{runnerup-flagvar|}}}}} }}

| label6     = 亞軍 <!-- Non Template:rl input -->
| data6      = {{#if:{{{runnerup_other|}}}|{{{runnerup_other|}}} }}

| data7      = {{#if:{{{attendance|}}}{{{points|}}}{{{tries|}}}{{{topscorer|}}}{{{top try scorer|}}}|<hr>}}

| label8     = 比賽場數
| data8      = {{{matches|}}}
| label9     = 總觀眾人數
| data9      = {{#if:{{{attendance|}}}|{{formatnum: {{{attendance|0}}}}}{{#if:{{{matches|}}}|&nbsp;<small>(每場 {{formatnum: {{#expr: {{{attendance|0}}} / {{{matches|1}}} round 0}}}})</small> }} }}

| label10     = 總得分
| data10      = {{#if:{{{points|}}}|{{{points}}}{{#if:{{{matches|}}}|&nbsp;<small>(每場 {{#expr: {{{points|0}}} / {{{matches|1}}} round 2}})</small>}} }}
| label11     = 總觸地次數
| data11      = {{#if:{{{tries|}}}|{{{tries}}}{{#if:{{{matches|}}}|&nbsp;<small>(每場 {{#expr: {{{tries|0}}} / {{{matches|1}}} round 2}})</small>}} }}

| label12     = 得分王{{#if:{{{topscorer2|}}}|s|}}
| data12      =  {{#if:{{{topscorer|}}}|{{flagicon|{{{topscorer-flag|}}}|{{{topscorer-flagvar|}}} }} {{{topscorer|}}}}}<!--
  -->{{#if:{{{topscorer2|}}}|<br />{{flagicon|{{{topscorer2-flag|}}}|{{{topscorer2-flagvar|}}} }} {{{topscorer2|}}}}}<!--
  -->{{#if:{{{topscorer3|}}}|<br />{{flagicon|{{{topscorer3-flag|}}}|{{{topscorer3-flagvar|}}} }} {{{topscorer3|}}}}}<!--
  -->{{#if:{{{topscorer4|}}}|<br />{{flagicon|{{{topscorer4-flag|}}}|{{{topscorer4-flagvar|}}} }} {{{topscorer4|}}}}}<!--
  -->{{#if:{{{topscorer5|}}}|<br />{{flagicon|{{{topscorer5-flag|}}}|{{{topscorer5-flagvar|}}} }} {{{topscorer5|}}}}}

| label13     = 觸地王{{#if:{{{top try scorer2|}}}|s|}}
| data13      = {{#if:{{{top try scorer|}}}|{{flagicon|{{{top try scorer-flag|}}}|{{{top try scorer-flagvar|}}} }} {{{top try scorer}}}}}<!--
  -->{{#if:{{{top try scorer2|}}}|<br />{{flagicon|{{{top try scorer2-flag|}}}|{{{top try scorer2-flagvar|}}} }} {{{top try scorer2}}}}}<!--
  -->{{#if:{{{top try scorer3|}}}|<br />{{flagicon|{{{top try scorer3-flag|}}}|{{{top try scorer3-flagvar|}}} }} {{{top try scorer3}}}}}<!--
  -->{{#if:{{{top try scorer4|}}}|<br />{{flagicon|{{{top try scorer4-flag|}}}|{{{top try scorer4-flagvar|}}} }} {{{top try scorer4}}}}}<!--
  -->{{#if:{{{top try scorer5|}}}|<br />{{flagicon|{{{top try scorer5-flag|}}}|{{{top try scorer5-flagvar|}}} }} {{{top try scorer5}}}}}

| belowstyle  = background:#eee;
| below       =  {{#if:{{{tournaments|}}}|{{align|center|'''[[世界盃聯盟式橄欖球賽]]'''}} }}<!--
  -->{{#if:{{{last|}}}|{{align|left| &nbsp;&lt;  {{{last|}}} }}}}<!--
  -->{{#if:{{{next|}}}|{{align|right|{{{next|}}} &gt;&nbsp;}} }}
}}<noinclude>{{documentation}}[[Category:聯盟式橄欖球|{{PAGENAME}}]]</noinclude>