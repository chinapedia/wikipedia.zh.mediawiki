! style="width:22%; text-align:center" | {{#if:{{{score|}}}|{{{score}}}|v }}{{#if:{{{gg|}}}|（[[黃金入球|g.g.]]）}}
! style="width:39%; text-align:left" class="vcard attendee" | <span class="fn org">{{{team2}}}</span>
|- style="vertical-align:top;font-size:85%"
| style="text-align:right"  | {{#if:{{{goals1|}}}|{{{goals1}}} }} 
| style="text-align:center" | {{#if:{{{report|}}}|{{{report}}} }} 
| style="text-align:left"   | {{#if:{{{goals2|}}}|{{{goals2}}} }}
{{#if:{{{penaltyscore|}}}|
{{!}}-
{{!}} &nbsp;
! style="text-align:center" {{!}} 点球
{{!}} &nbsp;
{{!}}- style=vertical-align:top;font-size:85%
{{!}} style="text-align:right"  {{!}} {{{penalties1}}}
! style="text-align:center" {{!}} {{{penaltyscore}}}
{{!}} style="text-align:left"   {{!}} {{{penalties2}}}
}}
|}
{| style="float:left; width:24%; table-layout:fixed"
|<div style="font-size: 85%">{{#if:{{{stadium|}}}|<span class="location">{{{stadium}}}</span> }}{{#if:{{{attendance|}}}|<br />观众人数：{{{attendance}}}}}{{#if:{{{referee|}}}|<br />裁判：{{{referee}}} }}</div>
|}{{clear}}
