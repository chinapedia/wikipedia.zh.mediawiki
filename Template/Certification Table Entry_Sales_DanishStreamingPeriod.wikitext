<includeonly>{{#if:{{{year|}}}
  |{{#ifexpr: {{{year}}} < 2011 <!-- Before 2011 -->
    |0
    |{{#ifexpr: {{{year}}} < 2012 <!-- year is 2011-->
      |{{#if:{{{month|}}}
        |{{#ifexpr: {{{month}}} < 2
          |{{#if:{{{day|}}}
            |{{#ifexpr: {{{day}}} < 7 | 0 | 1}}
            |0}}
          |1}}
        |0}}
      |{{#ifexpr: {{{year}}} < 2013 <!-- year is 2012-->
        |{{#if:{{{month|}}}
          |{{#ifexpr: {{{month}}} < 2
            |{{#if:{{{day|}}}
              |{{#ifexpr: {{{day}}} < 30 |1|2}}
              |0}}
            |{{#ifexpr: {{{month}}} < 4 
              |2
              |{{#ifexpr: {{{month}}} < 5
                |{{#if:{{{day|}}}
                  |{{#ifexpr: {{{day}}} < 30 |2|3}}
                  |0}}
                |3
                }}
              }}
            }}
          |0}}
        |{{#ifexpr: {{{year}}} < 2014 <!-- year is 2013-->
          |3
          |{{#ifexpr: {{{year}}} < 2015 <!-- year is 2014-->
            |{{#if:{{{month|}}}
              |{{#ifexpr: {{{month}}} < 3
                |3
                |{{#ifexpr: {{{month}}} < 4
                  |{{#if:{{{day|}}}
                    |{{#ifexpr: {{{day}}} < 21 |3|4}}
                    |0}}
                  |{{#ifexpr: {{{month}}} < 11
                    |4
                    |{{#ifexpr: {{{month}}} < 12
                      |{{#if:{{{day|}}}
                        |{{#ifexpr: {{{day}}} < 17 |4|0}}
                        |0}} 
                      |0}} <!-- December 2014 -->
                    }}
                  }}
                }}
              |0}}
            |0}} <!-- year is 2015+ -->          
          }}
        }}
      }}
    }}
  |0}}
</includeonly><noinclude>
{{Documentation}}
</noinclude>