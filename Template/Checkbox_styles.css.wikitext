.oo-ui-icon-check.oo-ui-image-invert.checkbox {
    display: inline-block;
    position: static;
    background-repeat: no-repeat;
    background-position: center center;
    border-width: 1px;
    border-style: solid;
    border-color: #72777d;
    border-radius: 2.5px;
    vertical-align: middle;
    margin: 5px 5px 5px 0;
    padding: 0;
    overflow: hidden;
    cursor: pointer;
    user-select: none;
}