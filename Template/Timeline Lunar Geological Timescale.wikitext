<timeline>
ImageSize  = width:800 height:75
PlotArea   = left:65 right:15 bottom:20 top:5
AlignBars  = justify

Colors =
  id:前酒海纪   value:rgb(0.823,0.302,0.380)
  id:酒海纪     value:rgb(0.898,0.329,0.071)
  id:雨海纪     value:rgb(0,0.481,0.811)
  id:早雨海世    value:rgb(0,0.404,0.741) 
  id:晚雨海世    value:rgb(0,0.557,0.882) 
  id:爱拉托逊纪   value:rgb(0.238,0.839,0.728) 
  id:哥伦布纪     value:rgb(0.5,0.5,0.5)

Period      = from:-4567.17 till:0
TimeAxis    = orientation:horizontal
ScaleMajor  = unit:year increment:500 start:-4500
ScaleMinor  = unit:year increment:100 start:-4500

PlotData=
  align:center textcolor:black fontsize:9 mark:(line,black) width:25 shift:(0,-5)

  bar:Period

  from: -1100   till:    0   text:[[哥伦布纪]]  color:哥伦布纪   
  from:-3200   till: -1100   text:[[爱拉托逊纪]]  color:爱拉托逊纪   
  from:-3850   till: -3200  text:[[雨海纪]]      color:雨海纪  
  from: -3920  till: -3850  text:[[酒海纪|N]] shift:(2,-5)       color:酒海纪
  from: start  till: -3920  text:[[前酒海纪]]       color:前酒海纪

  bar:Epoch

  from: -3200  till: 0       color:white
  from:  -3800 till: -3200   text:[[晚雨海世]]  color:晚雨海世
  from: -3850  till: -3800   text:[[早雨海世]] shift:(-20,-5)  color:早雨海世             
  from: start  till: -3850   color:white

</timeline>
