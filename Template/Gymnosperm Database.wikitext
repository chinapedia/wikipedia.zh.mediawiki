{{#if: {{{link|}}}
  | [{{Gymnosperm Database/url
    | family = {{{family|}}}
    | genus = {{{genus|}}}
    | species = {{{species|}}}
    | infraspecies = {{{subspecies|{{{variety|}}}}}}
    }} Gymnosperm Database]
  | {{#invoke: template wrapper | wrap | _template = cite web | _exclude = family, genus, species, subspecies, variety
    | url = {{Gymnosperm Database/url
      | family = {{{family|}}}
      | genus = {{{genus|}}}
      | species = {{{species|}}}
      | infraspecies = {{{subspecies|{{{variety|}}}}}}
      }}
    | title = {{#invoke: eFloras | name | 1 =
      {{{title|
        {{#if: {{{genus|}}}
          | {{#if: {{{species|}}}
            | {{#if: {{{subspecies|}}}
              | {{{genus}}} {{{species}}} subsp. {{{subspecies}}}
              | {{#if: {{{variety|}}}
                | {{{genus}}} {{{species}}} var. {{{variety}}}
                | {{{genus}}} {{{species}}}
                }}
              }}
            | {{{genus}}}
            }}
          | {{{family<noinclude>|Pinaceae</noinclude>}}}
          }}
        }}}
      }}
    | website = The Gymnosperm Database
    | editor-first = Christopher J.
    | editor-last = Earle
    | date = 2018
    }}
  }}{{#if: {{{family|<noinclude>Pinaceae</noinclude><includeonly>{{Gymnosperm Database/family| genus = {{{genus|}}} }}</includeonly>}}}
  |
  | {{error|{{para|family}} is required.}}
  }}<noinclude>
{{documentation}}
</noinclude>