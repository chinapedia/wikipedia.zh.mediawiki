<includeonly><!--The table wrapper below allows AI4-templated images to be viewed in mobile browsers-->
{| role="presentation" cellpadding="0" style="border-spacing:0; {{#switch: {{{align|}}} | left = clear:left; float:left; | center | centre = margin-left: auto; margin-right: auto; border: none; | right | #default = clear:right; float:right; }}"
|-<!--Annotated image 4 template code starts below here-->
|<div class="thumb {{#switch: {{{align|}}} | left = tleft" | center | centre = center | right | #default = tright}}" {{#switch: {{{align|}}}|centre|center=style="margin: 1em auto;|margin-top:0px; margin-bottom:0px;"}}>
<div class="thumbinner" style="width:{{#expr:{{{width|300}}}+2}}px;{{#if:{{{frameless|}}}|border:0px;background-color:#fff}} ">
{{#if:{{{header|}}}|
<div style="clear: both; font-weight: bold; font-size: 106.4%; text-align: {{{header_align|center}}}; background-color: {{{header_background|transparent}}};margin-bottom:3px;">{{{header}}}</div>
}}
<!--Start image wrapper-->
<div style="position:relative; width:{{{width|300}}}px; height:{{{height}}}px; overflow:hidden; border:{{#if:{{{frameless|}}}|0px|solid #ccc 1px}}; background-color:{{{image-bg-color|white}}};">
<!--Start annotated image-->
<div style="left:{{{image-left|0}}}px; top:{{{image-top|0}}}px; width:{{{image-width|300}}}px; position:absolute"> [[Image:{{{image}}}|{{{image-width|300}}}px|alt={{{alt|}}}|{{#ifeq:{{{nolink|}}}|yes|link=|link={{{link|COMMONS:File:{{{image}}}}}}}}]]
</div> <!-- end image -->
<!--Start annotations-->
<div style="text-align:{{{annot-text-align|center}}}; {{ifeq|{{{annot-font-size|}}}||line-height:110%;|font-size:{{{annot-font-size|}}}px; line-height:{{{annot-line-height|110%}}};}}">
<span style="background-color:{{{annot-background-color|transparent}}}; color:{{{annot-color|black}}}">{{{annotations}}}</span>
<!--annotations has syntax: {{annotations|fromLeft|fromTop|text}}-->
</div><!-- end annotations -->
</div><!-- end image wrapper -->
{{#if:{{{caption|}}}|<div class="thumbcaption" style="clear:left">{{#ifeq:{{{icon|}}}|none||<div style="float:left;margin-right:0.5em">[[File:interactive icon.svg|18px|link=|图像顶端包含可点击的链接|alt=图像顶端包含可点击的链接]]</div>}}{{{caption}}}{{#ifeq:{{{nolink|}}}|yes|&nbsp;[[Image:Desc-20.png|15px|link=COMMONS:File:{{{image}}}]]}}</div>}}
</div><!-- end thumbinner -->
</div><!-- end thumb -->
|}<!-- end table wrapper for mobile browsers --></includeonly><noinclude>
