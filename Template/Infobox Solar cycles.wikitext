<includeonly>{{infobox
| above        = {{#if:{{{cycle_num|}}}|''第{{{cycle_num}}}太陽週期''|使用上的說明，請參閱 [[Template:Infobox Solar cycles]]}}
| aboveclass   = 摘要
| abovestyle   = padding-bottom:0.25em; background:{{{background|{{{bgcolour|}}}}}}; line-height:1.2em; text-align:center; font-size:115%;
| image       = {{#switch:{{{image|}}}
                  |<!--blank-->
                  |blank       =
                  |???         = 
                  |#default    = [[File:{{{image}}}|frameless|upright=1|alt={{{Alt|}}}|{{#ifeq:{{{Border|}}}|yes|border}}]]
                }}
| imagestyle   = padding-bottom:0.5em;
| caption      = {{{caption|}}}
| captionstyle = padding-top:0.1em; line-height:1.2em; font-size:90%;
| headerstyle  = background:#FFBF00; font-size:105%;

| header1     = 太陽黑子資料
| label3      = 開始日期
|  data3      =  {{{start_date|}}}{{{startdate|}}}
| label4      = 結束日期
|  data4      = {{{end_date|}}}{{{enddate|}}}
| label5      = 持續時間（年）
|  data5      = {{{duration|}}}
| label6      = 最大值
|  data6      = {{{max_count|}}}
| label7      = 最大值月份
|  data7      = {{{max_count_date|}}}{{{max_date|}}}
| label8      = 最小值
|  data8      = {{{min_count|}}}
| label9      = 無黑子日數
|  data9      = {{{spotless_count|}}}{{{nospots|}}}{{{spotless_days|}}}

| header10    = 週期次序表
|  label11    = 上一週期
|   data11    = {{#switch:{{{prev_name|}}}
                  |<!--blank-->
                  |blank       =
                  |#default    = {{{prev_name}}} ({{{prev_dates}}})
                }}
|  label12    = 下一週期
|   data12    = {{#switch:{{{next_name|}}}
                  |<!--blank-->
                  |blank       =
                  |#default    = {{{next_name}}} ({{{next_dates}}})
                }}
}}</includeonly><noinclude>
{{documentation}}
</noinclude>