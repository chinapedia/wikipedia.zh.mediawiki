<table class="infobox vevent" cellpadding="3" style="width:24em; text-align:left; font-size:90%; line-height:1.5em; border-collapse:collapse;"><tr><td colspan="8" class="summary" style="text-align:center; font-size:125%; font-weight:bold; border-bottom:1px solid #aaa;"> {{#if:{{{year|}}}| 
      {{#ifexpr:{{{year}}} >= 1896| 
        {{#ifexpr:{{#expr:{{{year}}} mod 4}} = 0| 
          {{Roman|{{#expr:{{{year}}}/4-473}}}}屆夏季奧林匹克運動會自由車比賽 | 
          NOT an Summer Olympic year! }} | 
        Year not in modern era! }} | 
      No year specified!
    }}</td>
  </tr>
  {{#if:{{{image|}}}|
    {{#if:{{{caption|}}}|
      <tr>
        <td colspan="8" style="text-align:center; border-bottom:1px solid #aaa;"> {{{image}}} <br> <span style="font-size:95%">{{{caption}}}</span> </td>
      </tr>|
      <tr>
        <td colspan="8" style="text-align:center; border-bottom:1px solid #aaa;"> {{{image}}}
      </tr>|
    }}
  }}
  {{#if:{{{venues|}}}|
    <tr>
      <th> 場館 </th>
      <td colspan="7"> {{{venues}}} </td>
    </tr>
  }}
  {{#if:{{{venue|}}}|
    <tr>
      <th> Venue </th>
      <td colspan="7"> {{{venue}}} </td>
    </tr>
  }}
  {{#if:{{{BMXvenue|}}}{{{mountainbikevenue|}}}{{{roadvenue|}}}{{{trackvenue|}}} |
    <tr>
      <th> Venues </th>
    </tr>
    {{#if:{{{BMXvenue|}}}|
      <tr>
      <th style="text-align:left;"> <small>小輪車</small>  </th>
      <td colspan="7"> {{{BMXvenue}}} </td>
      </tr>
    }}
    {{#if:{{{mountainbikevenue|}}}|
      <tr>
      <th style="text-align:left;"> <small>登山車</small> </th>
      <td colspan="7"> {{{mountainbikevenue}}} </td>
      </tr>
    }}
    {{#if:{{{roadvenue|}}}|
      <tr>
      <th style="text-align:left;"> <small>公路賽</small>  </th>
      <td colspan="7"> {{{roadvenue}}} </td>
      </tr>
    }}
    {{#if:{{{trackvenue|}}}|
      <tr>
      <th style="text-align:left;"> <small>場地賽</small>  </th>
      <td colspan="7"> {{{trackvenue}}} </td>
      </tr>
    }}
  }}
  {{#if:{{{dates|}}}|
    <tr>
      <th> 日期 </th>
      <td colspan="7"> {{{dates}}} </td>
    </tr>
  }}
  {{#if:{{{cyclists|}}}|
    <tr>
      <th> Cyclists </th>
      <td colspan="7"> {{#if:{{{nations|}}}|{{{cyclists}}}&nbsp;from&nbsp;{{{nations}}}&nbsp;countries|{{{cyclists}}}}}</td>
    </tr>
  }}
  {{#if:{{{NOC1|}}}{{{NOC2|}}}{{{NOC3|}}}{{{NOC4|}}}{{{NOC5|}}}|
    <tr>
      <th colspan="8" style="border-top:1px solid #aaa;"> Top ranked countries </th>
    </tr>
    {{#if:{{{NOC1|}}}|
      <tr style="vertical-align:top">
        <th colspan="3" style="text-align:left"> {{nobreak|'''{{flagIOC|{{{NOC1}}}|{{{year}}} Summer}}:'''}} </th> 
        <td style="text-align:right"> {{{gold1}}}&nbsp;{{Gold01}}&nbsp; </td> <td style="text-align:right"> {{{silver1}}}&nbsp;{{Silver02}}&nbsp; </td> <td style="text-align:right"> {{{bronze1}}}&nbsp;{{Bronze03}} </td> <td>&nbsp;</td>
      </tr>
    }}
    {{#if:{{{NOC2|}}}|
      <tr style="vertical-align:top">
        <th colspan="3" style="text-align:left"> {{nobreak|'''{{flagIOC|{{{NOC2}}}|{{{year}}} Summer}}:'''}} </th> 
        <td style="text-align:right"> {{{gold2}}}&nbsp;{{Gold01}}&nbsp; </td> <td style="text-align:right"> {{{silver2}}}&nbsp;{{Silver02}}&nbsp; </td> <td style="text-align:right"> {{{bronze2}}}&nbsp;{{Bronze03}} </td> <td>&nbsp;</td>
      </tr>
    }}
    {{#if:{{{NOC3|}}}|
      <tr style="vertical-align:top">
        <th colspan="3" style="text-align:left"> {{nobreak|'''{{flagIOC|{{{NOC3}}}|{{{year}}} Summer}}:'''}} </th> 
        <td style="text-align:right"> {{{gold3}}}&nbsp;{{Gold01}}&nbsp; </td> <td style="text-align:right"> {{{silver3}}}&nbsp;{{Silver02}}&nbsp; </td> <td style="text-align:right"> {{{bronze3}}}&nbsp;{{Bronze03}} </td> <td>&nbsp;</td>
      </tr>
    }}
    {{#if:{{{NOC4|}}}|
      <tr style="vertical-align:top">
        <th colspan="3" style="text-align:left"> {{nobreak|'''{{flagIOC|{{{NOC4}}}|{{{year}}} Summer}}:'''}} </th> 
        <td style="text-align:right"> {{{gold4}}}&nbsp;{{Gold01}}&nbsp; </td> <td style="text-align:right"> {{{silver4}}}&nbsp;{{Silver02}}&nbsp; </td> <td style="text-align:right"> {{{bronze4}}}&nbsp;{{Bronze03}} </td> <td>&nbsp;</td>
      </tr>
    }}
    {{#if:{{{NOC5|}}}|
      <tr style="vertical-align:top">
        <th colspan="3" style="text-align:left"> {{nobreak|'''{{flagIOC|{{{NOC5}}}|{{{year}}} Summer}}:'''}} </th> 
        <td style="text-align:right"> {{{gold5}}}&nbsp;{{Gold01}}&nbsp; </td> <td style="text-align:right"> {{{silver5}}}&nbsp;{{Silver02}}&nbsp; </td> <td style="text-align:right"> {{{bronze5}}}&nbsp;{{Bronze03}} </td> <td>&nbsp;</td>
      </tr>
    }}
  }}
  {{hide in print|
    {{#if:{{{prev|}}}{{{next|}}}|
      {{#if:{{{year|}}}|
        {{#ifexpr:{{{year}}}<1900|
          <td style="text-align:left;border-top:1px solid #aaa; padding:0.2em;"> »&nbsp; </td> <td colspan="7" style="text-align:right;border-top:1px solid #aaa; padding:0.2em;"> {{{next}}}&nbsp;» </td>|
          {{#ifexpr:{{{year}}}>=2016|
            <td style="text-align:left;border-top:1px solid #aaa; padding:0.2em;"> «&nbsp;{{{prev}}} </td> <td colspan="7" style="text-align:right;border-top:1px solid #aaa; padding:0.2em;"> {{{next}}}&nbsp;» </td>|
            <td style="text-align:left;border-top:1px solid #aaa; padding:0.2em;"> «&nbsp;{{{prev}}} </td> <td colspan="7" style="text-align:right;border-top:1px solid #aaa; padding:0.2em;"> {{{next}}}&nbsp;» </td>
          }}
        }}
      }}
    }}
  }}
</table>
