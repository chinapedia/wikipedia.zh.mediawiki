{{#tag:timeline| 
DateFormat = mm/dd/yyyy
Define $now = {{CURRENTMONTH}}/{{CURRENTDAY2}}/{{CURRENTYEAR}}

Define $later = {{#time:m/d/Y|+13 months}}
Period = from:10/01/2001 till:$later
Define $skip = at:end # Force a blank line
Define $dayunknown = 15 # what day to use if it's actually not known
ImageSize= width:900 height:auto barincrement:20
TimeAxis = orientation:horizontal
PlotArea = right:1 left:1 bottom:65 top:1
Legend = orientation:vertical position:bottom columns:4

Colors =
     id:bg          value:white
     id:lightline   value:rgb(0.9,0.9,0.9)
     id:lighttext   value:rgb(0.5,0.5,0.5)
     id:server      value:rgb(0.8,0.8,0.9)
     id:gen1        value:rgb(0.95,0.95,1)
     id:gen2        value:rgb(0.9,0.9,1)
     id:gen3        value:rgb(0.85,0.85,1)
     id:gen4        value:rgb(0.8,0.8,1)
     id:photo       value:rgb(0.75,0.75,1)
     id:color       value:rgb(0.7,0.7,1)
     id:gen5        value:rgb(0.65,0.65,1)
     id:classic     value:rgb(0.6,0.6,1)
     id:gen7        value:rgb(0.55,0.55,1) Legend:iPod_Classic
     id:mini        value:rgb(0.9,1,0.8)
     id:mini2       value:rgb(0.8,1,0.8) Legend:iPod_Mini
     id:nano        value:rgb(0.7,0.9,0.8)
     id:nano2       value:rgb(0.6,0.9,0.8)
     id:nano3       value:rgb(0.5,0.9,0.8)
     id:nano4       value:rgb(0.4,0.9,0.8)
     id:nano5       value:rgb(0.3,0.9,0.8)
     id:nano6       value:rgb(0.2,0.9,0.8)
     id:nano7       value:rgb(0.1,0.9,0.8) Legend:iPod_Nano
     id:shuffle     value:rgb(1,0.9,0.4)
     id:shuffle2    value:rgb(1,0.8,0.4)
     id:shuffle3    value:rgb(1,0.7,0.4)
     id:shuffle4    value:rgb(1,0.6,0.4)
     id:shuffle5    value:rgb(1,0.5,0.4) Legend:iPod_Shuffle
     id:touch1      value:rgb(0.9,0.9,0.7)
     id:touch2      value:rgb(0.9,0.9,0.6)
     id:touch3      value:rgb(0.9,0.9,0.5)  
     id:touch4      value:rgb(0.9,0.9,0.4)
     id:touch5      value:rgb(0.9,0.9,0.3)
     id:touch6      value:rgb(0.9,0.9,0.2)
     id:touch7      value:rgb(0.9,0.9,0.1) Legend:iPod_Touch
     id:current     value:rgb(0.9,0.9,0.9) Legend:Still_produced

BackgroundColors = canvas:bg
ScaleMajor = gridcolor:lighttext unit:year increment:1 start:01/01/2002
ScaleMinor = gridcolor:lightline unit:month increment:3 start:10/01/2001

BarData = 
  barset:shuffle
  barset:minano
  barset:classic
  barset:touch

PlotData=
  width:15 textcolor:black shift:(5,-5) anchor:from fontsize:s

  barset:shuffle
    color:shuffle  from:01/11/2005 till:09/12/2006 text:"[[IPod shuffle|Shuffle 1G]]"
  barset:break
    color:shuffle2 from:09/12/2006 till:09/09/2009 text:"[[IPod shuffle|Shuffle 2G]]"
    color:shuffle3 from:03/11/2009 till:09/09/2010 text:"[[IPod shuffle|Shuffle 3G]]"
  barset:break
    color:shuffle4 from:09/09/2010 till:07/27/2017       text:"[[IPod shuffle|Shuffle 4G]]"

  barset:minano
    color:mini     from:01/06/2004 till:02/23/2005 text:"[[IPod mini|Mini 1G]]"
  barset:break
    color:mini2    from:02/23/2005 till:09/07/2005 text:"[[IPod mini|2G]]"
  barset:break
    color:nano     from:09/07/2005 till:09/12/2006 text:"[[IPod nano|Nano 1G]]"
  barset:break
    color:nano2    from:09/12/2006 till:09/05/2007 text:"[[IPod nano|Nano 2G]]"
  barset:break
    color:nano3    from:09/05/2007 till:09/09/2008 text:"[[IPod nano|Nano 3G]]"
  barset:break
    color:nano4    from:09/09/2008 till:09/09/2009 text:"[[IPod nano|Nano 4G]]"
  barset:break
    color:nano5    from:09/09/2009 till:09/01/2010 text:"[[IPod nano|Nano 5G]]"
  barset:break
    color:nano6    from:09/01/2010 till:10/12/2012 text:"[[IPod nano|Nano 6G]]"
  barset:break
    color:nano7    from:10/12/2012 till:07/27/2017       text:"[[IPod nano|Nano 7G]]"

  barset:classic
    color:gen1     from:10/23/2001 till:04/28/2003 text:"[[iPod Classic|Classic 1G]]"
    color:gen2     from:07/17/2002 till:04/28/2003 text:"[[iPod Classic|Classic 2G]]"
  barset:break
    color:gen3     from:04/28/2003 till:07/19/2004 text:"[[iPod Classic|Classic 3G]]"
    $skip
  barset:break
    color:gen4     from:07/19/2004 till:06/28/2005 text:"[[iPod Classic|Classic 4G]]"
    color:photo    from:10/26/2004 till:06/28/2005 text:"[[IPod classic|Photo]]"
  barset:break
    $skip
    color:color    from:06/28/2005 till:10/12/2005 text:"[[iPod Classic|Color]]"
  barset:break
    color:gen5     from:10/12/2005 till:09/05/2007 text:"[[iPod Classic|Classic 5G]]"
    $skip
  barset:break
    color:classic  from:09/05/2007 till:09/09/2014       text:"[[iPod Classic|Classic 6G]]"
    $skip
  barset:break


  barset:touch
    color:touch1   from:09/05/2007 till:09/09/2008 text:"[[IPod touch|Touch 1G]]"
  barset:break
    color:touch2   from:09/09/2008 till:09/01/2010 text:"[[IPod touch|Touch 2G]]"
    color:touch3   from:09/09/2009 till:09/01/2010 text:"[[IPod touch|Touch 3G]]"
  barset:break
    color:touch4   from:09/01/2010 till:05/30/2013 text:"[[IPod touch|Touch 4G]]"
  barset:break
    $skip
    color:touch5   from:10/11/2012 till:07/15/2015 text:"[[IPod touch (第五代)|Touch 5G]]"
  barset:break
    $skip
    color:touch6   from:07/15/2015 till:05/28/2019 text:"[[IPod touch (第六代)|Touch 6G]]"
  barset:break
    $skip
    color:touch7   from:05/28/2019 till:$now text:"[[IPod touch (第七代)|Touch 7G]]"
  barset:break
    $skip
    color:current  from:$now       till:end


}}
