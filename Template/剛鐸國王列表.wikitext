<div style="float: right;">
<timeline>
ImageSize  = width:150 height:800
PlotArea   = left:50 right:0 bottom:10 top:10
AlignBars  = early
Colors =
  id:linecolor   value:rgb(0.62,0.62,0.94)
  id:gc1      value:rgb(1,1,0.8) #groc clar
  id:gc2      value:rgb(1,1,0) #groc
  id:bl1      value:rgb(0.8,0.92,1) #blau clar
  id:bl2      value:rgb(0.4,0.6,1) #blau
  id:ve      value:rgb(0.8,1,0.8) #verd clar
  id:li      value:rgb(0.8,0.6,1) #lila  
  id:tr      value:rgb(1,0.8,0) #taronja
  id:gr1      value:rgb(0.86,0.86,0.86) #gris clar
  id:gr2      value:rgb(0.69,0.69,0.69) #gris fosc
  id:ro1      value:rgb(1,0.6,0.8) #rosa  
  id:ro2      value:rgb(1,0.93,0.8) #rosa capçalera
  id:sa      value:rgb(1,0.8,0.6) #salmo
  id:rg      value:red

DateFormat = yyyy
Period     = from:-121 till:2050
TimeAxis   = orientation:vertical order:reverse
ScaleMajor = unit:year increment:100 start:100

PlotData=
  bar:ReisGóndor width:100 mark:(line,white) align:center fontsize:S

  at:-121 shift:(4,-6) fontsize:7 text:
  from:-121  till:0 shift:(0,-3) color:gc2 text:[[伊蘭迪爾|Eléndil]]
  from:0  till:2 shift:(0,-3) color:tr text:[[埃西鐸|Isíldur]]
  from:2  till:158 shift:(0,-3) color:gc2 text:[[梅蘭迪爾|Menèldil]]
  from:158  till:238 shift:(0,-3) color:tr text:[[色曼多|Cemèndur]]
  from:238  till:324 shift:(0,-3) color:gc2 text:[[埃蘭迪爾 (剛鐸)|Eärendil]]
  from:324  till:411 shift:(0,-3) color:tr text:[[安拿迪爾|Anàrdil]]
  from:411  till:492 shift:(0,-3) color:gc2 text:[[奧斯托和|Ostoher]]
  from:491  till:541 shift:(0,-3) color:tr text:[[羅曼達希爾一世|Romendacil I]]
  from:541  till:667 shift:(0,-3) color:gc2 text:[[圖倫拔|Turàmbar]]
  from:667  till:748 shift:(0,-3) color:tr text:[[雅坦那塔一世|Atanatar I]]
  from:748  till:840 shift:(0,-3) color:gc2 text:[[西瑞安迪爾|Siriondil]]
  from:840  till:913 shift:(0,-3) color:tr text:[[塔拉農·法拉斯特|Tarannon Falastur]]
  from:913  till:936 shift:(0,-3) color:gc2 text:[[艾尼爾一世|Eärnil I]]
  from:936  till:1015 shift:(0,-3) color:tr text:[[色延迪爾|Ciryandil]]
  from:1015  till:1149 shift:(0,-3) color:gc2 text:[[海爾曼達希爾一世|Hyarmendacil I]]
  from:1149  till:1226 shift:(0,-3) color:tr text:[[雅坦那塔二世·亞卡林|Atanatar II Alcarin]]
  from:1226  till:1294 shift:(0,-3) color:gc2 text:[[那曼希爾一世|Narmacil I]]
  from:1294  till:1304 shift:(0,-3) color:tr text:[[卡馬希爾|Calmacil]]
  from:1304  till:1366 shift:(0,-3) color:gc2 text:[[羅曼達希爾二世|Romendacil II]]
  from:1366  till:1432 shift:(0,-2) color:tr text:[[瓦拉卡|Valacar]]
  from:1432  till:1437 shift:(0,-1) color:gc2 text:[[艾爾達卡 (剛鐸)|Eldacar]]
  from:1437  till:1447 shift:(0,-6) color:tr text:[[卡斯塔馬|Castamir]]
  from:1447  till:1490 shift:(0,-6) color:gc2 text:[[艾爾達卡 (剛鐸)|Eldacar]]
  from:1490  till:1540 shift:(0,-3) color:tr text:[[雅達墨|Aldamir]]
  from:1540  till:1621 shift:(0,-3) color:gc2 text:[[海爾曼達希爾二世|Hyarmendacil II]]
  from:1621  till:1634 shift:(0,-1) color:tr text:[[米那迪爾|Minardil]]
  from:1634  till:1636 shift:(0,-6) color:gc2 text:[[泰勒納|Telemnar]]
  from:1636  till:1798 shift:(0,-3) color:tr text:[[塔龍鐸 (剛鐸)|Tarondor]]
  from:1798  till:1850 shift:(0,-3) color:gc2 text:[[特路美泰·昂巴達希爾|Telumehtar]]
  from:1850  till:1856 shift:(0,-3) color:tr text:[[那曼希爾二世|Narmacil II]]
  from:1856  till:1936 shift:(0,-3) color:gc2 text:[[卡力美塔|Calimehtar]]
  from:1936  till:1944 shift:(0,-3) color:tr text:[[昂多赫|Ondoher]]
  from:1945  till:2043 shift:(0,-3) color:gc2 text:[[艾尼爾二世|Eärnil II]]
  from:2043  till:2050 shift:(0,-3) color:tr text:[[爾諾|Eärnur]]

</timeline>
</div>

