{{infobox
| bodyclass  = vcard
| bodystyle  = width: 22.5em;
| datastyle  = text-align: left;
| abovestyle = background: #{{#switch: {{{1}}}
| relative   = E8E4EF
| none       = E8E4EF
| 99BADD
}}; color: #{{#switch: {{{1}}}
| relative = 553377
| none     = 553377
| highness = 005566
| lesser   = 005566
| majesty  = 000066
| monarch  = 000066
| prince   = 000000
| princess = 000000
| royal    = 000000
| 000000
}}; 

| aboveclass = fn
| above      = {{{name<noinclude>|</noinclude>}}}

| subheader = {{{title|}}}

| image   = {{#invoke:InfoboxImage|InfoboxImage|image={{{image|}}}|size={{{image_size|}}}|sizedefault=frameless|upright=1|alt={{{alt|}}}}}
| caption = {{{caption|}}}

| headerstyle = background: #EEEEEE;
| header1     = {{#if: {{{succession|}}} | {{{succession}}} {{#switch:{{{more}}}|no=|{{#switch:{{{1}}}|monarch={{small|{{nobold|''[[Style of the British sovereign#List of changes to the royal style|(more...)]]''}}}}|majesty={{small|{{nobold|''[[Style of the British sovereign#List of changes to the royal style|(more...)]]''}}}} }} }} }}

| label3 = [[紋章]]
| data3  = {{{CoA|}}}

| label4 = {{if empty|{{{reign-type|}}}|{{#switch:{{{2}}}|consort=Consort|在位}}}}
| data4  = {{{reign|}}}

| label5 = {{if empty|{{{pre-type|}}}|前任}}
| data5  = {{{predecessor|}}}

| label6 = {{if empty|{{{suc-type|}}}|繼任}}
| data6  = {{{successor|}}}

| header10 = {{#if: {{{succession|}}} | <span style="height: 4px">&#32;</span>}}

| data11  = {{#if: {{{full name|}}} |
'''全名'''<div class="nickname" style="width:100%; padding-left:0.5em"><!-- "nickname" is the correct parameter value for hCard -->
{{{full name}}}</div>
}}

| label12 = 原文名
| data12  = {{#if:{{{native_name|}}}|<span class="nickname" {{#if:{{{native_name_lang|}}}|lang="{{{native_name_lang}}}"}}>-{{{{native_name}}}}-</span>}}

| data13  = {{#if: {{{styles|}}}{{{titles|}}}|
'''頭銜及榮譽'''<div style="width:100%; padding-left:0.5em">
{{#if:{{{styles|}}}|{{{styles}}}|{{{titles|}}}}}</div>
}}

| label14 = 出生
| data14  = {{Br separated entries|1={{#if:{{{birth_name|{{{birthname|}}}}}}|<span class="nickname">{{{birth_name|{{{birthname|}}}}}}</span>}}|2={{{birth_date|}}}|3={{#if:{{{birth_place|}}}|<span class="birthplace">{{{birth_place|}}}</span>}}}}

| label15 = 受洗
| data15  = {{br separated entries| {{if empty|{{{christening_date|}}}|{{{date of christening|}}}}} | {{if empty|{{{christening_place|}}}|{{{place of christening|}}}}} }}

| label16 = 逝世
| data16  = {{br separated entries|{{{death_date|}}}|{{{death_place|}}}}}

| label17 = 墓地
| data17  = {{br separated entries| {{if empty|{{{burial_date|}}}|{{{date of burial|}}}}} | {{#if:{{{burial_place|}}}{{{place of burial|}}}| <span class="label">{{if empty|{{{burial_place|}}}|{{{place of burial|}}}}}</span> }} | {{{resting_place_coordinates|}}} }}

| label18 = [[{{#if:{{{house-type|}}}|{{{house-type}}}|貴族}}|{{#if:{{{house-type|}}}|家族|貴族}}]]
| data18  = {{{noble family|}}}

| label19 = 配偶
| data19  = {{{spouse|}}}

| data20  = {{#if:{{{issue|}}} | 
{{#if:{{{issue-link|}}}|<div style="float: right; font-weight: bold;">{{small|''[[{{{issue-link}}}|{{#if:{{{issue-pipe|}}}|{{{issue-pipe}}}|Detail}}]]''}}</div> }}
'''子嗣'''<div style="width:100%; padding-left:0.5em">
{{{issue|}}}</div>
}}

| label25 = 父親
| data25  = {{{father|}}}

| label26 = 母親
| data26  = {{{mother|}}}

| label27 = 簽名
| data27  = {{{signature|}}}

| label28 = 宗教信仰
| data28  = {{{religion|}}}

| label29 = 職業
| data29  = {{{occupation|}}}

| label30 = 紀念碑
| data30  = {{{memorials|}}}

| label31 = 網站
| data31  = {{{website|{{{url|}}}}}}

| data32  = {{{module|}}}
}}<noinclude>
{{documentation}}
</noinclude>