<includeonly>{{infobox

| bodyclass = vcard
| bodystyle = {{WPMILHIST Infobox style|main_box_raw}}

| child = {{lc:{{{embed}}}}}
| title   = {{#ifeq:{{lc:{{{embed}}}}}|yes|'''纪念碑'''}}
| decat   = yes <!-- remove from template:infobox tracking categories -->

| aboveclass = fn org
| abovestyle = {{WPMILHIST Infobox style|header_raw}}
| above      = {{#ifeq:{{lc:{{{embed}}}}}|yes||
{{{name|{{PAGENAMEBASE}}}}}
}}

| subheaderstyle = {{WPMILHIST Infobox style|sub_header_raw}}

| subheader = {{#if:{{{native_name|}}} |<span class="nickname" {{#if:{{{native_name_lang|}}}|lang="{{{native_name_lang}}}"}}> {{{native_name}}} </span>}}
| subheader2 = {{#if:{{{body|}}} |{{{body}}} |{{{country}}} }}

| imagestyle = {{WPMILHIST Infobox style|image_box_plain_raw}}
| image = {{#invoke:InfoboxImage|InfoboxImage|image={{{image|}}}|size={{{image_size|}}}|sizedefault=frameless|upright=1.07|alt={{{alt|}}}}}
| caption = {{{caption|}}}

| headerstyle = {{WPMILHIST Infobox style|sub_header_raw}}
| labelstyle = padding-right:1.0em

| rowstyle1 = {{WPMILHIST Infobox style|sub_header_raw}}
|   data1 = {{#if:{{{commemorates|}}} |紀念對象：{{{commemorates}}} |紀念對象：{{{use_dates}}} }}

|  label2 = 建立
|   data2 = {{{established|}}}

|  label3 = 揭幕
|   data3 = {{{unveiled|}}}

|  label4 = 地點
|   data4 = {{br separated entries
             | 1 = {{{coordinates|}}}
             | 2 = {{#if:{{{location|}}}     |<span class="label">{{{location}}}</span>}}
             | 3 = {{#if:{{{nearest_town|}}} |<span class="label">{{{nearest_town}}}</span>附近}} 
            }}

|  rowclass5 = note
|  label5 = 設計者
|   data5 = {{{designer|}}}

|  label6 = 埋葬人數
|   data6 = {{{total_burials|{{{total|}}}}}}

|  label7 = 不知名的埋葬者
|   data7 = {{{unknowns|}}}

|  label8 = {{longitem|共纪念}}
|   data8 = {{{commemorated|}}}

| header9 = {{#if:{{{by_country|}}} |国家纪念碑}}
|   data10 = {{#if:{{{by_country|}}} |<div style="text-align:left;">
{{{by_country}}}</div>}}

| header11 = {{#if:{{{by_war|}}} |战争纪念碑}}
|   data12 = {{#if:{{{by_war|}}} |<div style="text-align:left;">
{{{by_war}}}</div>}}

|   data13 = {{#if:{{{inscription|}}}
              | <div style="border-top:1px solid #aaa;padding-top:0.2em;
                            padding-bottom:0.2em;border-bottom:1px solid #aaa;"> {{{inscription}}} </div>
             }}

|   data80 = {{{nrhp|{{{embedded|{{{module|}}}}}}}}}

| belowstyle = font-size:smaller;
| below = {{#if:{{{source|}}} |來源：{{{source|}}} }}

}}</includeonly><noinclude>
{{Documentation}}
</noinclude>