{{infobox
| bodyclass  = vcard
| bodystyle  = border-spacing: 3px; width: 289px; background:#e2ffea;

| labelstyle = padding: 0em 0.3em; background:#77cfcf;
| datastyle  = padding: 0em 0.3em 0em 0em;

| child      = {{{child|}}}

| aboveclass = fn org
| abovestyle = background:#0b9; color:white;
| above      = {{ifempty
               | {{{name|}}}
               | <includeonly>{{error|請輸入瀑布名稱|tag=span}}</includeonly>}}

| subheaderstyle = font-size: 110%;
| subheader      = {{{nativename|{{{alt_name|}}}}}}

| image1   = {{#if:{{{image|{{{photo|}}}}}}
             | [[file: {{{image|{{{photo|}}}}}}|{{least|{{{image_size|{{{photo_width|}}}}}}|275}}px<!--
               -->{{#if:{{{alt|{{{photo_alt|}}}}}}|{{!}}alt={{{alt|{{{photo_alt|}}}}}} }} ]] }}
| caption1 = {{{caption|{{{photo_caption|}}}}}}

| image2   = {{#if:{{{pushpin_map|}}}{{{map|}}}
             | <div class="center">{{Infobox map
               | map = {{{pushpin_map|{{{map|}}}}}}
               | map_alt = {{{pushpin_map_alt|{{{map_alt|}}}}}}
               | map_width = {{least|{{{pushpin_mapsize|{{{map_width|240}}}}}}|275}}
               | map_caption = {{{pushpin_map_caption|{{{map_caption|}}}}}}
               | relief = {{{relief|}}}
               | label = {{{pushpin_label|{{{map_label|}}}}}}
               | label_position = {{{pushpin_label_position|{{{map_label_position}}}}}}
               | mark = {{ifempty|{{{mark|}}}|Red pog.svg}}
               | lat = {{decdeg
                       | {{{latd|{{{lat_d|}}}}}}
                       | {{{latm|{{{lat_m|}}}}}}
                       | {{{lats|{{{lat_s|}}}}}}
                       | {{{latNS|{{{lat_NS|}}}}}} }}
               | long ={{decdeg
                       | {{{longd|{{{long_d|}}}}}}
                       | {{{longm|{{{long_m|}}}}}}
                       | {{{longs|{{{long_s|}}}}}}
                       | {{{longEW|{{{long_EW|}}}}}} }}
               | x = {{{x|}}}
               | y = {{{y|}}} }}</div> }}

| label1 = 所在地
| class1 = label
| data1  = {{{location|}}}

| label2 = [[:en:British national grid reference system|OS grid]]
| data2  = {{#if:{{{grid_ref_UK|}}}|{{gbm4ibx|{{{grid_ref_UK|}}}}} }}

| label3 = [[:en:Irish grid reference system|OSI/OSNI grid]]
| data3  = {{#if:{{{grid_ref_Ireland|}}}|{{iem4ibx|{{{grid_ref_Ireland|}}}}} }}

| label4 = 座標
| data4  = {{#if:{{{latd|{{{lat_d|}}}}}}{{{longd|{{{long_d|}}}}}}
           | {{Infobox coord
             | lat_d   = {{{latd|{{{lat_d|}}}}}}
             | lat_m   = {{{latm|{{{lat_m|}}}}}}
             | lat_s   = {{{lats|{{{lat_s|}}}}}}
             | lat_NS  = {{{latNS|{{{lat_NS|}}}}}}
             | long_d  = {{{longd|{{{long_d|}}}}}}
             | long_m  = {{{longm|{{{long_m|}}}}}}
             | long_s  = {{{longs|{{{long_s|}}}}}}
             | long_EW = {{{longEW|{{{long_EW|}}}}}}
             | type    = {{ifempty|{{{coords_type|}}}|waterbody}}
             | region  = {{{region|}}}
             | dim     = {{{dim|}}}
             | scale   = {{ifempty|{{{scale|}}}|50000}}
             | source  = {{{source|}}}
             | format  = {{{format|}}}
             | display = {{{display|}}}
             | name    = <!-- leave undefined --> }}{{{coordinates_ref|{{{coords_ref|}}}}}}
           | {{ifempty|{{{coordinates|}}}|{{{coords|}}} }}{{{coordinates_ref|{{{coords_ref|}}}}}}
           }}

| class5 = category
| label5 = 分類
| data5  = {{{type|}}}

| rowclass6 = note
| label6 = 海拔
| data6  = {{{elevation|}}}

| rowclass7 = note
| label7 = 高度
| data7  = {{{height|}}}

| rowclass8 = note
| label8 = 瀑布數
| data8  = {{{number_drops|}}}

| label9  = 最大落差
| data9   = {{{height_longest|}}}

| rowclass10 = note
| label10= 寬度
| data10  = {{{width|}}}

| label11 = 平均寬度
| data111 = {{{average_width|}}}

| rowclass12 = note
| label12 = 水系
| data12  = {{{watercourse|}}}

| rowclass13 = note
| label13 = 平均流速
| data13  = {{{average_flow|}}}

| label14 = 世界落差排名
| data14  = {{{world_rank|}}}

| data15 = {{{embedded|}}}
}}<noinclude>{{Documentation|Template:Infobox_waterfall/doc}}</noinclude>