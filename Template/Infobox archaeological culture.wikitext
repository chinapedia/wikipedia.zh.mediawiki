{{Infobox
| bodyclass = vcard

| child = {{#ifeq:{{{embed|}}}|yes|yes}}

| title = <includeonly>{{#if:{{{name|}}}|{{{name}}}|{{PAGENAMEBASE}}}}</includeonly>
| titleclass = fn org

| subheader = {{#if:{{{native_name|}}}|<div class="nickname" {{#if:{{{native_name_lang|}}}|lang="{{{native_name_lang}}}"}}>{{{native_name}}}</div>}}

| headerstyle = background-color: #eee;

| image   = {{#invoke:InfoboxImage|InfoboxImage|image={{{map|}}}|size={{{mapsize|}}}|upright={{{mapupright|1}}}|alt={{{mapalt|<includeonly>{{PAGENAME}}的範圍</includeonly>}}}}}
| caption = {{{mapcaption|}}}
| labelstyle = white-space: nowrap;
| label1  = 其他名稱
|  data1  = {{{altnames|}}}
| label2  = 考古層
|  data2  = {{{horizon|}}}
| label3  = 地理範圍
|  data3  = {{{region|}}}
| label4  = 時期
|  data4  = {{{period|}}}
| label5  = 時間
|  data5  = {{{dates|}}}
| label6  = 典型代表
|  data6  = {{{typesite|}}}
| label7  = 主要遺址
|  data7  = {{{majorsites|}}}
| label8  = 特色
|  data8  = {{{extra|}}}
| label9  = 先前文化
|  data9  = {{{precededby|}}}
| label10 = 繼承文化
|  data10 = {{{followedby|}}}
|  data11 = {{{module|}}}
}}<noinclude>
