{{Infobox
|bodyclass = vevent
| bodystyle      = border-collapse:collapse;{{#if:{{{dissipated|}}}|<!--if-->|<!--not-->}}
|title = {{if empty|{{{name|}}}|{{{event|}}}|<includeonly>{{PAGENAMEBASE}}</includeonly>}}

<!-- 龍捲風 -->
| above = {{#switch: {{lc:{{{tornadoes|}}}}} |0|1|one|={{#switch: {{lc:{{{fujitascale}}}}} 
|f0|f1|f2|f3|f4|f5|f0+|f1+|f2+|f3+|f4+= [[藤田級數|{{uc:{{{fujitascale}}}}}級]]龍捲風
|ef0|ef1|ef2|ef3|ef4|ef5|ef0+|ef1+|ef2+|ef3+|ef4+= [[改良藤田級數|{{uc:{{{fujitascale}}}}}級]]龍捲風}} }}

| abovestyle = background-color:{{#switch: {{lc:{{{fujitascale}}}}}
|f0|ef0|f0+|ef0+=#{{Storm colour|storm}}
|f1|ef1|f1+|ef1+=#{{Storm colour|cat1}}
|f2|ef2|f2+|ef2+=#{{Storm colour|cat2}}
|f3|ef3|f3+|ef3+=#{{Storm colour|cat3}}
|f4|ef4|f4+|ef4+=#{{Storm colour|cat4}}
|f5|ef5=#{{Storm colour|cat5}} }}

|subheaderstyle = padding:0;

<!-- Type header -->
|subheader1style = background:#{{Storm colour|{{#switch:{{{maintype}}}
  |extratropical cyclone = post-tropical
  |subtropical cyclone = storm
  |tropical storm = storm
  |medicane = storm
  |tropical cyclone = cat1
  |#default = 
  }}
}}
|subheader1 = {{#if:{{{maintype|}}}
  |<div style="font-weight:bold; padding:0.1em; background:#{{Storm colour|{{#switch:{{{maintype}}}|extratropical cyclone = post-tropical|subtropical cyclone = storm|tropical storm = storm|medicane = storm|tropical cyclone = cat1|#default = }}}}">{{#switch:{{{maintype}}}
    |extratropical cyclone = [[溫帶氣旋]] ([[薩菲爾-辛普森颶風風力等級|SSHWS]])
    |subtropical cyclone = [[亞熱帶氣旋]] ([[薩菲爾-辛普森颶風風力等級|SSHWS]])
    |tropical storm = [[熱帶風暴]] ([[薩菲爾-辛普森颶風風力等級|SSHWS]])
    |medicane = [[地中海颶風]]
    |tropical cyclone = [[熱帶氣旋]] ([[薩菲爾-辛普森颶風風力等級|SSHWS]])
  }}</div>
}}

<!-- RSI (美國冬季風暴) -->
|subheader2 = {{#if:{{{RSI|}}}|<div style="font-weight:bold;padding:0.1em;background-color:#{{Storm colour|{{#ifexpr:{{{RSI}}}>=3|{{#ifexpr:{{{RSI}}}>=6|{{#ifexpr:{{{RSI}}}>=10|{{#ifexpr:{{{RSI}}}>=18|cat5|cat4}}|cat3}}|cat2}}|cat1}} }}">分類&nbsp;{{#ifexpr:{{{RSI}}}>=3|{{#ifexpr:{{{RSI}}}>=6|{{#ifexpr:{{{RSI}}}>=10|{{#ifexpr:{{{RSI}}}>=18|5&nbsp;"極強"|4&nbsp;"強"}}|3&nbsp;"中"}}|2&nbsp;"弱"}}|1&nbsp;"偏弱"}}&nbsp;([[區域降雪指數|RSI]]/[[NOAA]]:&nbsp;{{{RSI}}})</div>}}

<!-- RSI2 (美國冬季風暴) -->
|subheader3 = {{#if:{{{RSI2|}}}|<div style="font-weight:bold;padding:0.1em;background-color:#{{Storm colour|{{#ifexpr:{{{RSI2}}}>=3|{{#ifexpr:{{{RSI2}}}>=6|{{#ifexpr:{{{RSI2}}}>=10|{{#ifexpr:{{{RSI2}}}>=18|cat5|cat4}}|cat3}}|cat2}}|cat1}} }}">分類&nbsp;{{#ifexpr:{{{RSI2}}}>=3|{{#ifexpr:{{{RSI2}}}>=6|{{#ifexpr:{{{RSI2}}}>=10|{{#ifexpr:{{{RSI2}}}>=18|5&nbsp;"極強"|4&nbsp;"強"}}|3&nbsp;"中"}}|2&nbsp;"弱"}}|1&nbsp;"偏弱"}}&nbsp;([[區域降雪指數|RSI]]/[[NOAA]]:&nbsp;{{{RSI2}}})</div>}}


