| publisher = Environment Canada
| access-date = 14 May 2016}}</ref>和1950年6月18日<ref name="June 1950">{{cite web
| url = http://climate.weather.gc.ca/climate_data/daily_data_e.html?hlyRange=1953-01-01%7C1976-01-29&dlyRange=1897-07-01%7C1979-01-31&mlyRange=1897-01-01%7C1979-12-01&StationID=1534&Prov=YT&urlExtension=_e.html&searchType=stnName&optLimit=yearRange&StartYear=1840&EndYear=2016&selRowPerPage=25&Line=1&searchMethod=contains&txtStationName=dawson&timeframe=2&Year=1950&Month=6#
